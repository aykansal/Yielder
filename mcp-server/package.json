{
  "name": "dex-mcp",
  "version": "1.0.0",
  "description": "Model Context Protocol server for accessing DEX pool data from Permaswap and Botega on AO network",
  "main": "dist/index.js",
  "bin": {
    "dex-mcp": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "inspect": "npx @modelcontextprotocol/inspector node ./dist/index.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "dex",
    "pools",
    "permaswap",
    "botega",
    "ao",
    "arweave",
    "yield-farming",
    "liquidity-pools"
  ],
  "author": "aykansal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aykansal/yielder.git",
    "directory": "mcp"
  },
  "homepage": "https://github.com/aykansal/yielder/tree/main/mcp#readme",
  "bugs": {
    "url": "https://github.com/aykansal/yielder/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@permaweb/aoconnect": "^0.0.90",
    "arweave": "^1.15.7",
    "dex-mcp": "^1.0.0",
    "mcps-logger": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
